{
  "private": true,
  "name": "remnote-mcp-bridge",
  "version": "0.4.1",
  "license": "MIT",
  "scripts": {
    "check-types": "tsc",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --color --progress --no-open",
    "build": "npx remnote-plugin validate && shx rm -rf dist && cross-env NODE_ENV=production webpack --color --progress && cd dist && bestzip ../PluginZip.zip ./*",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "typecheck": "tsc --noEmit",
    "lint": "npx eslint src test --ext .ts,.tsx",
    "lint:fix": "npx eslint src test --ext .ts,.tsx --fix",
    "format": "npx prettier --write \"src/**/*.{ts,tsx}\" \"test/**/*.{ts,tsx}\"",
    "format:check": "npx prettier --check \"src/**/*.{ts,tsx}\" \"test/**/*.{ts,tsx}\"",
    "quality": "./code-quality.sh",
    "precommit": "./code-quality.sh"
  },
  "dependencies": {
    "@remnote/plugin-sdk": "latest",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.7",
    "@types/node": "^25.2.1",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.2",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitest/coverage-v8": "^1.6.1",
    "@vitest/ui": "^1.6.1",
    "autoprefixer": "^10.4.7",
    "bestzip": "^2.2.1",
    "concurrently": "^7.2.2",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "esbuild-loader": "^2.19.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.2",
    "happy-dom": "^12.10.3",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.6.1",
    "postcss": "^8.4.14",
    "postcss-import": "^14.1.0",
    "postcss-loader": "^7.0.0",
    "react-refresh": "^0.14.0",
    "shx": "^0.3.4",
    "style-loader": "^3.3.1",
    "tailwindcss": "^3.1.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4",
    "vitest": "^1.6.1",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.9.3"
  }
}
